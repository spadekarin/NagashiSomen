using System.Collections;
using System.Collections.Generic;
using UnityEngine;
//Escキーで終了
public class esc : MonoBehaviour
{
    // Start is called before the first frame update
    void Start()
    {
        
    }

    
// Update is called once per frame
void Update()
    {
        if (Input.GetKey(KeyCode.Escape))
        {
            Application.Quit();
        }
    }

    
}
